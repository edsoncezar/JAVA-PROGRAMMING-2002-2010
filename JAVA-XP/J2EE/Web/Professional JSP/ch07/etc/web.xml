<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.3.dtd">

<web-app>

    <servlet>
        <servlet-name>
            BabyGameServlet
        </servlet-name>
        <servlet-class>
            com.wrox.projsp.ch07.BabyGameServlet
        </servlet-class>
    </servlet>

    <servlet>
        <servlet-name>
            BabyGameServletSW
        </servlet-name>
        <servlet-class>
            com.wrox.projsp.ch07.BabyGameServletSW
        </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>
            BabyGameServlet
        </servlet-name>
        <url-pattern>
            /BabyGameServlet/*
        </url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>
            BabyGameServletSW
        </servlet-name>
        <url-pattern>
            /BabyGameServletSW/*
        </url-pattern>
    </servlet-mapping>

</web-app>
